<%= paginator.render do -%>
  <ul class="pagination">
    <%= prev_page_tag %>

    <% each_page do |page| -%>
      <% if page.left_outer? %>
        <%= page_tag page %>
        <%= gap_tag %>
      <% elsif page.inside_window? %>
        <%= page_tag page %>
      <% elsif page.right_outer? %>
        <%= gap_tag %>
        <%= page_tag page %>
      <% end %>      
    <% end -%>

    <%= next_page_tag %>
  </ul>
<% end -%>
